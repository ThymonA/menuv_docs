{
    "template": {
        "options": {
            "title": {
                "label": "MenuV Docs",
                "href": "."
            },
            "logo": {
                "dark": "img/menuv.png",
                "light": "img/menuv.png"
            },
            "sidebar": {
                "enabled": true,
                "outline": "tree",
                "collapsed": false,
                "toolbar": true,
                "itemsFolded": false,
                "itemsOverflow": "crop",
                "badges": true,
                "search": true,
                "animations": true
            },
            "symbols": {
                "autoLink": true,
                "params": "list",
                "enums": "list",
                "props": "list",
                "meta": false
            },
            "contentView": {
                "bookmarks": "h1,h2,h3",
                "faLibs": "all"
            },
            "navbar": {
                "enabled": true,
                "fixed": true,
                "dark": false,
                "animations": true,
                "menu": [
                    {
                        "label": "Documentation",
                        "href": "api/"
                    },
                    {
                        "iconClass": "fab fa-lg fa-github",
                        "label": "",
                        "href": "https://github.com/ThymonA/menuv",
                        "target": "_blank"
                    }
                ]
            }
        }
    },
    "jsdoc": {
        "encoding": "utf8",
        "recurse": true,
        "pedantic": true,
        "access": "public",
        "package": null,
        "module": true,
        "undocumented": false,
        "undescribed": true,
        "hierarchy": true,
        "sort": "kind",
        "filter": null,
        "allowUnknownTags": true
    },
    "debug": true,
    "markdown": {
        "gfm": true,
        "tables": true,
        "breaks": true,
        "pedantic": true,
        "sanitize": true,
        "smartLists": true,
        "smartypants": true,
        "xhtml": true,
        "tasks": true,
        "emoji": true
    },
    "app": {
        "title": "MenuV Documentation",
        "meta": null,
        "routing": "path",
        "server": "github",
        "favicon": "./img/favicon.ico"
    },
    "src": [
        "./docs/*.md",
        "./docs/**/*.md",
        "./src/*.js",
        "./src/**/*.js",
        "./src/**/*.ts"
    ],
    "assets": {
        "/img": [ "./img/*.png", "./img/*.ico" ]
    },
    "dest": "./output/docs",
    "clean": true
}